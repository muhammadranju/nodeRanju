<!-- Navbar-->


<div class="container mt-4 pb-3">
    <div class="bg-light box-shadowf-lg  overflow-hidden">
        <div class="row">

            <!-- Sidebar-->
            <%- include('./userSidebar/userSidebar.ejs') %>
            <!-- Sidebar-->

            <section class="col-lg-10 pt-lg-4 pb-4 mb-3">
                <div class=" px-4 ps-lg-0 pe-xl-5">
                    <!-- Title-->
                    <div class="d-sm-flex flex-wrap justify-content-between align-items-center border-bottom">
                        <h2 class="font-weight-bold py-2 text-center text-sm-left">My Downloads</h2>
                        <div class="py-2">
                            <div class="d-flex flex-nowrap align-items-center pb-3">
                            </div>
                        </div>
                    </div>
                    <div class="container pb-5 mb-2 mb-md-4">
                        <div class="row pt-3 mx-n2">
                            <% for(let i=0;i<4;i++){ %>
                            <!-- Products list-->
                            <div class="card product-card  col-lg-3 col-md-4 col-sm-4 mt-2">
                                <a class="card-img-top d-block" href="/posts/products">
                                    <img data-src="/images/marketplace/products/10.jpg"
                                        src="/images/lazyload/pikslide-lazyload.jpg" class="card-img-top mt-3 lazyload"
                                        alt="Product">
                                </a>
                                <div class="card-body ">
                                    <div class="d-flex flex-wrap justify-content-between align-items-start pb-1">
                                        <div class="text-muted font-size-xs mr-1">by
                                            <a class="product-meta font-weight-bold" href="/users/profile">
                                                Pikslide</a>
                                        </div>
                                        <div class="star-rating">
                                            <i class="sr-star czi-star-filled active"></i>
                                            <i class="sr-star czi-star-filled active"></i>
                                            <i class="sr-star czi-star-filled active"></i>
                                            <i class="sr-star czi-star-filled active"></i>
                                            <i class="sr-star czi-star-filled active"></i>
                                        </div>
                                    </div>
                                    <h3 class="product-title font-size-sm mb-2">
                                        <a href="/posts/products">Floating Phone and Tablet...</a>
                                    </h3>
                                </div>
                                <div class="d-flex flex-wrap justify-content-between align-items-center mb-3">
                                    <div class="mr-2">
                                        <button class="btn btn-danger btn-shadow btn-sm" type="button"><i
                                                class="czi-heart font-size-lg mr-2"></i>Like</button>
                                        <!-- <button class="btn btn-primary btn-sm btn-shadow" type="button">Save </button> -->
                                    </div>
                                    <div class="">
                                        <a class="social-link sb-sm ml-2" href="#">
                                            <i class="czi-share-alt font-size-lg align-middle text-muted mr-3"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <% } %>
                        </div>
                    </div>

                    <!-- Pagination-->
                    <nav class="d-md-flex justify-content-between align-items-center text-center text-md-start mt-4"
                        aria-label="Page navigation">
                        <div class="d-md-flex align-items-center w-100"><span
                                class="fs-sm text-muted me-md-3 mr-2">Showing 5 of 9 products</span>
                            <div class="progress w-100 my-3 mx-auto mx-md-0" style="max-width: 10rem; height: 4px;">
                                <div class="progress-bar" role="progressbar" style="width: 56%;" aria-valuenow="56"
                                    aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </div>
                        <button class="btn btn-outline-primary btn-sm" type="button">More products</button>
                    </nav>
                </div>
            </section>
        </div>
