<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Pikslide | Fashion Store v.1</title>
    <!-- SEO Meta Tags-->
    <meta name="description" content="Pikslide - Bootstrap E-commerce Template">
    <meta name="keywords"
        content="bootstrap, shop, e-commerce, market, modern, responsive,  business, mobile, bootstrap 4, html5, css3, jquery, js, gallery, slider, touch, creative, clean">
    <meta name="author" content="Pikslide Studio">
    <!-- Viewport-->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Style Link -->
    <%-include('../../Link_Script/Links.ejs')%>
    <!-- Style Link -->
</head>

<body class="toolbar-enabled">

    <!-- Sign in / sign up modal-->
    <%- include('../../Components/Login&SignUpSection') %>
    <!-- Sign in / sign up modal-->

    <!-- Quick View Modal-->
    <%- include('../../Components/QuickViewModalSection.ejs') %>
    <!-- Quick View Modal-->

    <!-- Navbar-->
    <%- include('../../Components/NavbarSection')  %>
    <!-- Navbar--




    <!-- Main Section -->
    <!-- --------------------------------------------------------------- -->


    <!-- Home icon -->
    <div class="container mt-3 mb-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item">
                    <a href="/">
                        <i class="czi-home"></i>
                        Home
                    </a>
                </li>
                <li class="breadcrumb-item">
                    <a href="/posts">Category</a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">Product</li>
            </ol>
        </nav>

        <!-- Carousel Section -->
        <!-- Multiple items + Static controls outside + No dots + Loop (Responsive) -->
        <div class="cz-carousel cz-controls-sm cz-controls-static cz-controls-outside mt-3">
            <div class="cz-carousel-inner"
                data-carousel-options='{"items": 3, "nav": false, "responsive": {"0":{"items":1},"500":{"items":2, "gutter": 18},"768":{"items":3, "gutter": 20}, "1100":{"gutter": 24}}}'>
                <a href="#">
                    <img src="../../../assets/img/components/carousel/04.jpg" alt="...">
                </a>
                <a href="#">
                    <img src="../../../assets/img/components/carousel/03.jpg" alt="...">
                </a>
                <a href="#">
                    <img src="../../../assets/img/components/carousel/02.jpg" alt="...">
                </a>
                <a href="#">
                    <img src="../../../assets/img/components/carousel/01.jpg" alt="...">
                </a>
                <a href="#">
                    <img src="../../../assets/img/components/carousel/05.jpg" alt="...">
                </a>


            </div>
        </div>

    </div>
    <!-- Carousel Section End-->


    <!-- Page Content-->
    <%- include('../../Components/HomeProductViewSection') %>
    <!-- Page Content-->

    <!-- Pagination Section -->
    <div class="container pb-5 mb-2 mb-md-4">
        <nav aria-label="Page navigation example">
            <ul class="pagination justify-content-center">
                <li class="page-item">
                    <a href="#" class="page-link" aria-label="Previous">
                        <i class="czi-arrow-left"></i>
                    </a>
                </li>
                <li class="page-item d-sm-none">
                    <span class="page-link page-link-static">2 / 5</span>
                </li>
                <li class="page-item d-none d-sm-block">
                    <a href="#" class="page-link">1</a>
                </li>
                <li class="page-item active d-none d-sm-block" aria-current="page">
                    <span class="page-link">
                        2
                        <span class="sr-only">(current)</span>
                    </span>
                </li>
                <li class="page-item d-none d-sm-block">
                    <a href="#" class="page-link">3</a>
                </li>
                <li class="page-item d-none d-sm-block">
                    <a href="#" class="page-link">4</a>
                </li>
                <li class="page-item d-none d-sm-block">
                    <a href="#" class="page-link">5</a>
                </li>
                <li class="page-item">
                    <a href="#" class="page-link" aria-label="Next">
                        <i class="czi-arrow-right"></i>
                    </a>
                </li>
            </ul>
        </nav>
    </div>
    <!-- Pagination Section End -->

    <!-- Main Section End -->
    <!-- --------------------------------------------------------------- -->








    <!-- Toast: Added to Cart-->
    <%- include('../../Components/miniComponents/Add_to_Cart') %>
    <!-- Toast: Added to Cart-->

    <!-- Footer-->
    <%- include('../../Components/FooterSection') %>
    <!-- Footer-->

    <!-- Mobile Viewport -->
    <%- include('../../Components/miniComponents/MobileViewSection') %>
    <!-- Mobile Viewport -->

    <!-- Vendor scrits: js libraries and plugins-->
    <%- include('../../Link_Script/Script') %>
    <!-- Vendor scrits: js libraries and plugins-->


</body>

</html>